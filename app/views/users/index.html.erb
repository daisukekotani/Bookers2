<h1>Users</h1>
<% @users.each do |user| %>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><span><%= user.id %></span></td>
        <td><span><%= user.name %></span></td>
        <td><span><%= link_to "Show",user_path(user.id)%></span></td>
      </tr>
  </tbody>
</table>
<% end %>